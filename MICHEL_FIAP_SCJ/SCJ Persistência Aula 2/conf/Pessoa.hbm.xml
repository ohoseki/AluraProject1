<hibernate-mapping>

	<class name="br.fiap.Pessoa" table="Pessoa" catalog="FIAP">
		<id name="id" column="ID" type="int">
			<generator class="native" />
		</id>
		<property name="nome" column="nome" type="string" length="200"/>
		<property name="apelido" column="apelido" type="string" length="200"/>
		<property name="endereco" column="endereco" type="string" length="200"/>
	</class>

</hibernate-mapping>